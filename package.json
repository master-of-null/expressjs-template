{
  "name": "expressjs",
  "version": "1.0.0",
  "main": "index.js",
  "author": "master-of-null <gabriel@digital-dash.co>",
  "license": "MIT",
  "scripts": {
    "format": "prettier . --ignore-unknown --write",
    "lint": "eslint . --format=pretty --quiet",
    "dev": "NODE_ENV=development nodemon src/index.js | pino-pretty",
    "setupenv": "cp -n .env.example .env",
    "db:migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "db:rollback": "knex migrate:rollback --knexfile db/knexfile.js",
    "db:seed": "knex seed:run --knexfile db/knexfile.js",
    "test": "jest --runInBand --forceExit",
    "docker:test": "docker-compose run --rm node-app yarn test"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@vscode/sqlite3": "^5.0.8",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
    "knex": "^1.0.5",
    "pg": "^8.7.3",
    "pino": "^7.9.2",
    "pino-http": "^6.6.0"
  }
}
